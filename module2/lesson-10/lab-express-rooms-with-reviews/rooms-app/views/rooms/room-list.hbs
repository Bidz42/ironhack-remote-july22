<ul>
  {{#each rooms}}
  <li> 
    <h3> <a href="/rooms/{{this._id}}"> {{ this.name }} </a></h3>
    <img src="{{this.imageUrl}}" alt="room_image" width="300">
    <p>- posted by {{ this.owner.fullName }}</p>
    {{#if this.isOwner}}
      <form action="/rooms/{{this._id}}/delete" method="POST">
        <button type="submit">Delete</button>
      </form>
    {{/if}}
  </li>
  {{/each}}
</ul>